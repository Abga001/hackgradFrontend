{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/media/*.{png,jpg,jpeg,gif,bmp,webp,svg}",
      "/static/css/*",
      "/static/js/*",
      "/*.{css,scss,js,json,ico,woff,woff2}"
    ]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "mimeTypes": {
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  }
}